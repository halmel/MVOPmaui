<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="skolaApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:skolaApp"
        xmlns:Views="clr-namespace:skolaApp.Views"
    Shell.FlyoutBehavior="Disabled">
    <FlyoutItem Title="Home">
        <Tab>
            <ShellContent
        Title="Zamestanci"
        ContentTemplate="{DataTemplate Views:Zamestanci}"
        Route="MainPage" />
            <ShellContent
        Title="Ucitele"
        ContentTemplate="{DataTemplate Views:Ucitele}"
        Route="Ucitele" />
            <ShellContent
        Title="Udrzbari"
        ContentTemplate="{DataTemplate Views:Udrzbari}"
        Route="Udrzbari" />
            <ShellContent
        Title="Podpora"
        ContentTemplate="{DataTemplate Views:Podpora}"
        Route="Podpora" />
            <ShellContent
        Title="Studenti"
        ContentTemplate="{DataTemplate Views:Studenti}"
        Route="Studenti" />
        </Tab>
    </FlyoutItem>


</Shell>
